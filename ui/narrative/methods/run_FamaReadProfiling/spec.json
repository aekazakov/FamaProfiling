{
    "ver": "1.0.1",
    "authors": [
        "kazakov"
    ],
    "contact": "http://www.kbase.us/support",
    "categories": ["active","communities"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "read_library_refs",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseAssembly.PairedEndLibrary","KBaseFile.PairedEndLibrary","KBaseAssembly.SingleEndLibrary","KBaseFile.SingleEndLibrary"]
            }
        },
       {
            "id" : "ref_dataset",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "nitrogen" ],
            "field_type" : "dropdown",
            "dropdown_options":{
              "options": [
                {
                  "value": "nitrogen",
                  "display": "Nitrogen cycle genes, v.1.4",
                  "id": "nitrogen",
                  "ui_name": "nitrogen"
                },
                {
                  "value": "universal",
                  "display": "Universal single-copy marker genes, v.1.4",
                  "id": "universal",
                  "ui_name": "universal"
                },
                {
                  "value": "rpl6",
                  "display": "Ribosomal protein L6, v.1.4",
                  "id": "rpl6",
                  "ui_name": "rpl6"
                }
              ]
            }
        },
        {
            "id" : "is_paired_end",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ 1 ],
            "field_type" : "dropdown",
            "dropdown_options":{
                "options": [{
                    "value": 1,
                    "display": "Paired End"
                }, {
                    "value": 0,
                    "display": "Single End"
                }]
            }            
        },
        {
            "id" : "output_read_library_name",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "Fama.filtered.reads" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : ["KBaseFile.SingleEndLibrary", "KBaseFile.PairedEndLibrary"],
                "is_output_name":true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "FamaProfiling",
            "method": "run_FamaReadProfiling",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "read_library_refs",
                    "target_property": "read_library_refs",
                     "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "ref_dataset",
                    "target_property": "ref_dataset"
                },
                {
                    "input_parameter": "is_paired_end",
                    "target_property": "is_paired_end"
                },
                {
                    "input_parameter": "output_read_library_name",
                    "target_property": "output_read_library_name"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
