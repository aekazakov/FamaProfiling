{
    "ver": "1.0.1",
    "authors": [
        "kazakov"
    ],
    "contact": "aekazakov@lbl.gov",
    "categories": ["active","sequence"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "genome_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseGenomes.Genome", "KBaseSets.GenomeSet", "KBaseSearch.GenomeSet"]
            }
        },
       {
            "id" : "ref_dataset",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "nitrogen" ],
            "field_type" : "dropdown",
            "dropdown_options":{
              "options": [
                {
                  "value": "nitrogen",
                  "display": "Nitrogen cycle genes, v.1.4",
                  "id": "nitrogen",
                  "ui_name": "nitrogen"
                },
                {
                  "value": "universal",
                  "display": "Universal single-copy marker genes, v.1.4",
                  "id": "universal",
                  "ui_name": "universal"
                },
                {
                  "value": "rpl6",
                  "display": "Ribosomal protein L6, v.1.4",
                  "id": "rpl6",
                  "ui_name": "rpl6"
                }
              ]
            }
        },
        {
            "id" : "output_feature_set_name",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "Fama.filtered.features" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ "KBaseCollections.FeatureSet" ],
                "is_output_name":true
            }
        },
        {
            "id" : "output_annotation_name",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "Fama.annotations." ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ "KBaseGeneFamilies.DomainAnnotation" ],
                "is_output_name":true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "FamaProfiling",
            "method": "run_FamaGenomeProfiling",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "genome_ref",
                    "target_property": "genome_ref",
                     "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "ref_dataset",
                    "target_property": "ref_dataset"
                },
                {
                    "input_parameter": "output_feature_set_name",
                    "target_property": "output_feature_set_name"
                },
                {
                    "input_parameter": "output_annotation_name",
                    "target_property": "output_annotation_name"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
